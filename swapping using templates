#include<iostream>
using namespace std;
template<class T>
void swapping(T &a,T &b)
{
	T temp;
	temp=a;
	a=b;
	b=temp;
}
int main()
{
	int a,b;
	cout<<"\nenter a and b:";
	cin>>a>>b;
	cout<<"\nbefore swapping:";
	cout<<"a="<<a<<"\t"<<"b="<<b;
	swapping(a,b);
	cout<<"\nafter swapping:";
	cout<<"a="<<a<<"\t"<<"b="<<b;
	char ch1,ch2;
	cout<<"\nenter ch1 and ch2:";
	cin>>ch1>>ch2;
	cout<<"\nbefore swapping:";
	cout<<"ch1="<<ch1<<"\t"<<"ch2="<<ch2;
	swapping(ch1,ch2);
	cout<<"\nafter swapping:";
	cout<<"ch1="<<ch1<<"\t"<<"ch2="<<ch2;
	return 0;
}
